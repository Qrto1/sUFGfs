local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
 
local Window = OrionLib:MakeWindow({Name = "Qrto All Scripts", HidePremium = false, SaveConfig = true, IntroText = "Qrto Hub", ConfigFolder = "Qrto"})
 
local RandomTab = Window:MakeTab({
	Name = "Blox Fruit",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
 
local Section = RandomTab:AddSection({
	Name = "Sub To Qrto"
})
 
